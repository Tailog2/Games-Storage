@{
    ViewData["Title"] = "NewGames";
}

<h1>New Game</h1>
<div>
    <form id="newGame" style="font-size: 20px">
        <div class="form-group">
            <label class="form-label">Game</label>
            <div class="tt-container">
                <input name="customer" id="customer" type="text" value="" class="typeahead"/>
            </div>
        </div>
        <div class="form-group">
            <label class="form-label">Movie</label>
            <div class="tt-container">
                <input name="movie" id="movie" type="text" value="" class="typeahead"/>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4 col-sm-4">
                <ul id="movies" class="list-group"></ul>
            </div>
        </div>
        <button class="btn btn-primary">Submit</button>
    </form>
</div>
